<!DOCTYPE html>
<html>
  <head>
    <title>DesafioBackendMaino</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>
  <body>
    <%= render 'shared/notice/alerts' %>
    <div class="container-fluid">
      <div class="row">
        <%= render "shared/header" %>
        <div style="display: grid; grid-template-columns: 225px calc(100% - 225px);">
        <%= render "shared/sidebar" %>
        <main class="col">
          <%= yield %>
        </main>
        </div>
      </div>
    </div>
  </body>
</html>
